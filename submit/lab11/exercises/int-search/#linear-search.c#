/** Return index of element in a[nElements]; < 0 if not found. */
int
search_for_element(int a[], int nElements, int element)
{
  int flag = 0;
  for(int i = 0; i < nElements; i++) {
    if(a[i] == element) {
      flag = i;
    }
    else {
      flag = -1;
    }
    return flag;
    
  }
  //@TODO add your code here to meet above spec.
}
